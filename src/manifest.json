{
  "name": "Poloniex Yen View",
  "version": "0.2.0",
  "description": "Add \"USD Value\", \"JPY Value\", \"BTC Rate\" columns into Poloniex Balances view.",
  "manifest_version": 2,
  "content_scripts": [{
    "js": [
      "script.js"
    ],
    "matches": [
      "https://poloniex.com/balances"
    ],
    "run_at": "document_start"
  }],
  "permissions": [
    "https://coincheck.com/api/ticker",
    "https://poloniex.com/balances"
  ]
}
